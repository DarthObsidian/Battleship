.cell {
    width: 9.09%;
    height: 9.09%;
    border-left-color: rgb(0, 100, 0);
    border-right-color: rgb(0, 100, 0);
    border-top-color: rgb(0, 100, 0);
    border-bottom-color: rgb(0, 100, 0);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    align-items: center;
    justify-content: center;
}

.radarButton {
    flex-direction: column;
    -unity-background-scale-mode: scale-to-fit;
    background-image: url('project://database/Assets/Art/Sprites/Radar.png?fileID=21300000&guid=799d4089613e31249957513064ba1fea&type=3#Radar');
    background-color: rgba(188, 188, 188, 0);
    border-left-color: rgba(149, 149, 149, 0);
    border-right-color: rgba(149, 149, 149, 0);
    border-top-color: rgba(149, 149, 149, 0);
    border-bottom-color: rgba(149, 149, 149, 0);
    flex-grow: 0;
    height: 30%;
}

.radarButton:hover {
    -unity-background-image-tint-color: rgb(192, 192, 192);
}

.radarButton:active {
    -unity-background-image-tint-color: rgb(9, 176, 29);
}

.okayButton {
    position: absolute;
    right: 30%;
    bottom: 5%;
    left: 30%;
    -unity-font-style: bold;
    background-color: rgb(178, 178, 178);
    color: rgb(20, 0, 255);
}

.okayButton:hover {
    background-color: rgb(147, 147, 147);
}

.okayButton:active {
    background-color: rgb(103, 102, 102);
}
